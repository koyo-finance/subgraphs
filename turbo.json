{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"generate": {
			"outputs": ["generated", "subgraph.yaml", "src/constants/index.ts"]
		},
		"build": {
			"outputs": ["build/**"],
			"dependsOn": ["generate", "$NETWORK"]
		},
		"test": {
			"outputs": []
		}
	},
	"globalDependencies": ["$NETWORK"]
}
